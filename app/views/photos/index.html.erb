<h1>Say Cheese</h1>
<h4><em>A photo world where cheese is MORE than just a cheese.</em></h4>

<% if current_user != nil %>
<p><%= link_to "Add Photo", new_photo_path %></p>

<p><%= link_to "Go to User Page", user_path(current_user) %></p>
<% end %>
  <% @photos.each do |photo| %>
    <%= link_to image_tag(photo.image.url(:medium)), photo_path(photo) %>
  <% end %>
