<h1><%= @photo.name %></h1>
<br></br>
<%=image_tag(@photo.image.url(:medium)) %>

<p><%= link_to "Tag People", new_photo_tag_path(@photo) %></p>

<p><%= link_to "Like It!", new_photo_favorite_path(@photo) %></p>

<p><%= link_to "Home Page", photos_path %></p>

<% if current_user != nil && current_user.id == @photo.user_id %>
  <p><%= link_to "Delete", photo_path(@photo), method: "delete" %></p>
<% end %>
