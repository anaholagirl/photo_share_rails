<h1> <%= current_user.email %> Profile </h1>

<p>Tagged pictures:</p>
<% current_user.tagged_photos.each do |photo| %>
  <%= link_to image_tag(photo.image.url(:medium)), photo_path(photo) %>
<% end %>
<br></br>
<p>Liked photos:</p>
<% current_user.favorited_photos.each do |photo| %>
  <%= link_to image_tag(photo.image.url(:medium)), photo_path(photo) %>
<% end %>

<p><%= link_to "Home Page", photos_path, :class => 'btn btn-success' %></p>
